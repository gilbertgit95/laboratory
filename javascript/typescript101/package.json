{
  "name": "api-core-a-test",
  "version": "1.0.0",
  "description": "Kagiweb api core (type A). The main purpose of this repo is tobe an api base code application, so we dont need to create a new app from scratch everytime we develop a web api",
  "homepage": "https://github.com/gilbertgit95/kagiweb-api-core-a#readme",
  "keywords": [
    "api basecode",
    "express app",
    "typescript express"
  ],
  "main": "build/src/index.js",
  "scripts": {
    "start": "node build/src/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p ./tsconfig.json",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "apidoc": "swagger-cli bundle docs/swaggerDocs/index.yml --outfile docs/swaggerDocs/__swaggerDocsBuild.json"
  },
  "author": {
    "name": "Gilbert D. Cuerbo",
    "email": "gilbert.cuerbo@gmail.com",
    "url": "https://gilbertgit95.github.io/"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "jest": "^29.5.0",
    "mongodb-memory-server": "^8.13.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "moment": "^2.29.4",
    "mongoose": "^7.2.0",
    "swagger-themes": "^1.2.30",
    "swagger-ui-express": "^4.6.3",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  }
}
